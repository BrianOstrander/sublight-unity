    "encyclopediaLogs": [
      {
        "entries": [
          {
            "entry": {
              "encyclopediaId": "dfc21640-2976-47fe-bdd1-d2048fa2cd98",
              "title": "LSM Rosa",
              "header": "Fate",
              "body": "Requiring additional fuel, your crew salvaged what they could from the wreck of the LSM Rosa. The ark will drift in orbit around the blackhole, until it too gets consumed by the void one day.",
              "orderWeight": -1
            }
          }
        ],
        "index": 17,
        "ending": true,
        "logId": "7d349f95-919f-464a-ab74-8ff75009eb92",
