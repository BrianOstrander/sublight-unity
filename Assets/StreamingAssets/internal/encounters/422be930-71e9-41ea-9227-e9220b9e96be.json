        },
        "header": "Captain Gail Colt",
        "message": "LSM Beethoven greets you! Warmest regards!",
        "nextLogId": "c1c6b6a6-b6ef-40b8-a7bb-fbedfd4ec04e",
        "index": 1,
        "logId": "4f2f4c3c-3d52-4392-afe7-65837d49b578"
